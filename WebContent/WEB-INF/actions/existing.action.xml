<?xml version="1.0" encoding="ISO-8859-1" ?>
<actions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schemas/action.xsd">
    <action>
        <type>existing</type>
        <version>1</version>
        <name>Existing</name>	        
	    <class>com.rapid.actions.Existing</class>
<!-- 

This action runs actions that already exist on other controls		
		
-->	    	
		<properties>
	        <property>
	            <key>action</key>
	            <name>Action</name>
	            <changeValueJavaScript>select</changeValueJavaScript>
	            <getValuesFunction>
	                <![CDATA[
return "<option value=''>None</option>" + getExistingActionOptions(this.action, this.id);	                
	                ]]>
	            </getValuesFunction>	 
	        </property>
	    </properties>
  
    </action> 
</actions>